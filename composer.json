{
  "name": "milocalidad/core-bundle",
  "type": "symfony-bundle",
  "description": "Mi Localidad Core Bundle",
  "require": {
    "php": "^8.2",
    "symfony/console": "^6.2",
    "symfony/symfony": "6.2.*"
  },
  "autoload": {
    "psr-4": {
      "MiLocalidad\\CoreBundle\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true ,
  "autoload-dev": {
    "psr-4": {
      "MiLocalidad\\CoreBuncle\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "license": "private",
  "require-dev": {
    "symfony/maker-bundle": "^1.48"
  }
}
